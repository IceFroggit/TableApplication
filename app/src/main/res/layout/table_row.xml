<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TableLayout
        android:id="@+id/table_heading_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/table_border">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/member_name"
                style="@style/table_row_item"
                android:textSize="@dimen/_9ssp"
                android:layout_width="0dp"
                android:layout_column="0"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="Name"/>
            <TextView
                android:id="@+id/member_id"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="1"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <TextView
                android:id="@+id/grade_1"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="1"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <EditText
                android:id="@+id/grade_2"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="2"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center"/>
            <EditText
                android:id="@+id/grade_3"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="3"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <EditText
                android:id="@+id/grade_4"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="4"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <EditText
                android:id="@+id/grade_5"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="5"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <EditText
                android:id="@+id/grade_6"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="6"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <EditText
                android:id="@+id/grade_7"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="7"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="1"
                android:textAlignment="center" />
            <TextView
                android:id="@+id/sum_of_points"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="8"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="Sum"
                android:textAlignment="center" />
            <TextView
                android:id="@+id/place"
                android:enabled="true"
                style="@style/table_row_item"
                android:layout_width="0dp"
                android:layout_column="9"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:text="Place"
                android:textAlignment="center" />
        </TableRow>
    </TableLayout>

</LinearLayout>